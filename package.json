{
  "name": "annonskollen-root",
  "version": "1.0.0",
  "scripts": {
    "start:node": "cd backend && npm run dev",
    "start:worker": ". .venv/bin/activate && export PYTHONPATH=$PYTHONPATH:. && python -m app.worker",
    "dev": "concurrently -n \"NODE,WORKER\" -c \"green,magenta\" \"npm run start:node\" \"npm run start:worker\""},
  "dependencies": {
    "@hono/swagger-ui": "^0.5.3",
    "@hono/zod-openapi": "^1.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
